        <command name='FL'       format='m,nX'        exec='exec_FL'        />
        <command name='FM'       format='@X///'       exec='exec_FM'        />
        <command name='FN'       format='m,n:@X///'   exec='exec_FN'        />
        <command name='FQ'       format='@Xq//'       exec='exec_FQ'        />
        <command name='FR'       format='m,n:@X//'    exec='exec_FR'        />
        <command name='FS'       format='m,n::@X///'  exec='exec_FS'        />
        <command name='FU'       format='m,nX'        exec='exec_FU'        />
        <command name='FZ'                            exec='exec_FZ'        />
        <command name='F_'       format='m,n:@X///'   exec='exec_F_ubar'    />
        <command name='F|'       format='X$'          exec='exec_F_vbar'    />
    </section>
</teco>

