<?xml version="1.0" encoding="utf-8"?>
<teco name="teco" version="200">
    <section title="commands">

        <!-- General commands -->

        <!--
             Format attribute defines command modifiers and arguments. "X"
             stands for the specific command (e.g., ^A or FS).

             Exec attribute defines the function to be called for the command.

             If format is not defined, the command is an operator or simple
             operand.

             If format ends with "!", that signifies a command that returns
             a value if and only if it is not preceded by an expression.

             If format ends with "$", that signifies a command that accepts
             m and n arguments, but neither uses them nor passes them through.
        -->

        <!--     name='NUL'      (illegal command)    -->
        <command name='CTRL_A'   format='m,n:@X//'    exec='exec_ctrl_A'    />
        <command name='CTRL_B'                        exec='exec_ctrl_B'    />
        <command name='CTRL_C'   format='X$'          exec='exec_ctrl_C'    />
        <command name='CTRL_D'   format='X'           exec='exec_ctrl_D'    />
        <command name='CTRL_E'   format='m,nX!'       exec='exec_ctrl_E'    />
        <!--     name='CTRL_F'   (illegal command)    -->
        <!--     name='CTRL_G'   (illegal command)    -->
        <command name='CTRL_H'                        exec='exec_ctrl_H'    />
        <command name='CTRL_I'   format='X//'         exec='exec_ctrl_I'    />
        <!--     name='LF'       (whitespace)         -->
        <!--     name='VT'       (whitespace)         -->
        <!--     name='FF'       (whitespace)         -->
        <!--     name='CR'       (whitespace)         -->
        <command name='CTRL_N'   format='nX'          exec='exec_ctrl_N'    />
        <command name='CTRL_O'   format='X'           exec='exec_ctrl_O'    />
        <command name='CTRL_P'                        exec='exec_ctrl_P'    />
        <command name='CTRL_Q'   format='nX'          exec='exec_ctrl_Q'    />
        <command name='CTRL_R'   format='nX!'         exec='exec_ctrl_R'    />
        <command name='CTRL_S'                        exec='exec_ctrl_S'    />
        <command name='CTRL_T'   format='m,n:X'       exec='exec_ctrl_T'    />
        <command name='CTRL_U'   format='m,n:@Xq//'   exec='exec_ctrl_U'    />
        <command name='CTRL_V'   format='nX'          exec='exec_ctrl_V'    />
        <command name='CTRL_W'   format='nX'          exec='exec_ctrl_W'    />
        <command name='CTRL_X'   format='nX!'         exec='exec_ctrl_X'    />
        <command name='CTRL_Y'                        exec='exec_ctrl_Y'    />
        <command name='CTRL_Z'                        exec='exec_ctrl_Z'    />
        <command name='ESC'      format='X$'          exec='exec_escape'    />
        <!--     name='\x1C'     (illegal command)    -->
        <!--     name='\x1D'     (illegal command)    -->
        <!--     name='\x1E'     (special character)  -->
        <command name='\x1F'                          exec='exec_ctrl_ubar' />
        <!--     name='SPACE'    (whitespace)         -->
        <command name='!'        format='m,n@X//'     exec='exec_bang'      />
        <command name='"'        format='nX'          exec='exec_quote'     />
        <command name='#'                             exec='exec_oper'      />
        <!--     name='$'        (illegal command)    -->
        <command name='%'        format='n:Xq'        exec='exec_pct'       />
        <command name='&amp;'                         exec='exec_oper'      />
        <command name='&apos;'   format='X$'          exec='exec_apos'      />
        <command name='('                             exec='exec_lparen'    />
        <command name=')'                             exec='exec_rparen'    />
        <command name='*'                             exec='exec_oper'      />
        <command name='+'                             exec='exec_oper'      />
        <command name=','                             exec='exec_comma'     />
        <command name='-'                             exec='exec_oper'      />
        <command name='.'                             exec='exec_dot'       />
        <command name='/'                             exec='exec_oper'      />
        <command name='0'                             exec='exec_number'    />
        <command name='1'                             exec='exec_number'    />
        <command name='2'                             exec='exec_number'    />
        <command name='3'                             exec='exec_number'    />
        <command name='4'                             exec='exec_number'    />
        <command name='5'                             exec='exec_number'    />
        <command name='6'                             exec='exec_number'    />
        <command name='7'                             exec='exec_number'    />
        <command name='8'                             exec='exec_number'    />
        <command name='9'                             exec='exec_number'    />
        <!--     name=':'        (command modifier)   -->
        <command name=';'        format='n:X'         exec='exec_semi'      />
        <command name='&lt;'     format='nX'          exec='exec_lt'        />
        <command name='='        format='n:@X//'      exec='exec_equals'    />
        <command name='&gt;'     format='X$'          exec='exec_gt'        />
        <command name='?'        format='X'           exec='exec_trace'     />
        <!--     name='@'        (command modifier)   -->
        <command name='A'        format='n:X'         exec='exec_A'         />
        <command name='B'                             exec='exec_B'         />
        <command name='C'        format='n:X'         exec='exec_C'         />
        <command name='D'        format='m,n:X'       exec='exec_D'         />
        <!--     name='E'        (E commands)         -->
        <!--     name='F'        (F commands)         -->
        <command name='G'        format='n:Xq'        exec='exec_G'         />
        <command name='H'                             exec='exec_H'         />
        <command name='I'        format='m,n@X//'     exec='exec_I'         />
        <command name='J'        format='n:X'         exec='exec_J'         />
        <command name='K'        format='m,nX'        exec='exec_K'         />
        <command name='L'        format='n:X'         exec='exec_L'         />
        <command name='M'        format='m,n:Xq'      exec='exec_M'         />
        <command name='N'        format='n:@X//'      exec='exec_N'         />
        <command name='O'        format='n@X//'       exec='exec_O'         />
        <command name='P'        format='m,n:X'       exec='exec_P'         />
        <command name='Q'        format='n:Xq'        exec='exec_Q'         />
        <command name='R'        format='n:X'         exec='exec_R'         />
        <command name='S'        format='m,n::@X//'   exec='exec_S'         />
        <command name='T'        format='m,n:X'       exec='exec_T'         />
        <command name='U'        format='m,nXq'       exec='exec_U'         />
        <command name='V'        format='m,nX'        exec='exec_V'         />
        <command name='W'        format='m,n:X'       exec='exec_W'         />
        <command name='X'        format='m,n:Xq'      exec='exec_X'         />
        <command name='Y'        format=':X'          exec='exec_Y'         />
        <command name='Z'                             exec='exec_Z'         />
        <command name='['        format='m,nX'        exec='exec_lbracket'  />
        <command name='\'        format='nX'          exec='exec_bslash'    />
        <command name=']'        format='m,n:X'       exec='exec_rbracket'  />
        <!--     name='^'        (special character)  -->
        <command name='_'        format='n:@X//'      exec='exec_ubar'      />
        <!--     name='`'        (illegal command)    -->
        <!--     name='{'        (illegal command)    -->
        <command name='|'        format='X$'          exec='exec_vbar'      />
        <!--     name='}'        (illegal command)    -->
        <!--     name='~'        (illegal command)    -->
        <!--     name=DEL        (illegal command)    -->

        <!-- E commands -->

        <command name='E%'       format=':@Xq//'      exec='exec_E_pct'     />
        <command name='E1'       format='m,nX!'       exec='exec_E1'        />
        <command name='E2'       format='m,nX!'       exec='exec_E2'        />
        <command name='E3'       format='m,nX!'       exec='exec_E3'        />
        <command name='E4'       format='m,nX!'       exec='exec_E4'        />
        <command name='EA'       format='X'           exec='exec_EA'        />
        <command name='EB'       format=':@X//'       exec='exec_EB'        />
        <command name='EC'       format='nX'          exec='exec_EC'        />
        <command name='ED'       format='m,nX!'       exec='exec_ED'        />
        <command name='EE'       format='nX!'         exec='exec_EE'        />
        <command name='EF'       format='X'           exec='exec_EF'        />
        <command name='EG'       format='::@X//'      exec='exec_EG'        />
        <command name='EH'       format='m,nX!'       exec='exec_EH'        />
        <command name='EI'       format=':@X//'       exec='exec_EI'        />
        <command name='EJ'       format='m,n:X'       exec='exec_EJ'        />
        <command name='EK'       format='X'           exec='exec_EK'        />
        <command name='EL'       format=':@X//'       exec='exec_EL'        />
        <command name='EN'       format=':@X//'       exec='exec_EN'        />
        <command name='EO'       format='n:X!'        exec='exec_EO'        />
        <command name='EP'       format='X'           exec='exec_EP'        />
        <command name='EQ'       format=':@Xq//'      exec='exec_EQ'        />
        <command name='ER'       format=':@X//'       exec='exec_ER'        />
        <command name='ES'       format='m,nX!'       exec='exec_ES'        />
        <command name='ET'       format='m,nX!'       exec='exec_ET'        />
        <command name='EU'       format='nX!'         exec='exec_EU'        />
        <command name='EV'       format='m,nX!'       exec='exec_EV'        />
        <command name='EW'       format=':@X//'       exec='exec_EW'        />
        <command name='EX'       format='X'           exec='exec_EX'        />
        <command name='EY'       format=':X'          exec='exec_EY'        />
        <command name='E_'       format='n:@X//'      exec='exec_E_ubar'    />

        <!-- F commands -->

        <command name='F&apos;'  format='X$'          exec='exec_F_apos'    />
        <command name='F0'                            exec='exec_F0'        />
        <command name='F1'       format='m,n@X///'    exec='exec_F1'        />
        <command name='F2'       format='m,n@X///'    exec='exec_F2'        />
