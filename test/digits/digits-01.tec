! TECO test: Test digits !
! Commands: 0-9 !
! Requirements: None !
! Execution: Standard !
! Expect: PASS [digits-01.log] !

EO - 100 "L
    -1 EJ - 1024 "E                 ! TECO-32 !
        3 UZ                        ! CR/LF, -P !
    |                               ! Unknown or unsupported TECO !
        @^A/Unknown TECO: EO=/
        EO :=
        @^A/, -1EJ=/
        -1EJ =
        ^C
    '
|
    EO - 200 "L                     ! TECO C !
        0 UZ                        ! LF, no -P !
    |                               ! TECO-64 !
        7 UZ                        ! LF, -P, ext. features !
    '
'

QZ&1 "N
    13@^UY//
    @^UZ/ 13^T/
'

10:@^UY//
:@^UZ/ 10^T/

@^A/Testing base 10/ ^D MZ

^D 10^R

 00 UA QA:@=/%d /                   ! Test: positive decimal digits !
 11 UA QA:@=/%d /
 22 UA QA:@=/%d /
 33 UA QA:@=/%d /
 44 UA QA:@=/%d /
 55 UA QA:@=/%d /
 66 UA QA:@=/%d /
 77 UA QA:@=/%d /
 88 UA QA:@=/%d /
 99 UA QA:@=/%d /
 -1 UA QA @=/%d/                    ! Test: negative no. in base 10 !

@^A/Testing base 8/ ^D MZ

^D 8^R

 00 UA QA:@=/%o /                   ! Test: positive octal digits !
 11 UA QA:@=/%o /
 22 UA QA:@=/%o /
 33 UA QA:@=/%o /
 44 UA QA:@=/%o /
 55 UA QA:@=/%o /
 66 UA QA:@=/%o /
 77 UA QA:@=/%o /
 -1 UA QA @=/%o/                    ! Test: negative no. in base 8 !

@^A/Testing base 16/ ^D MZ

^D 16^R

 00 UA QA:@=/%x /                   ! Test: positive hexadecimal digits !
 11 UA QA:@=/%x /
 22 UA QA:@=/%x /
 33 UA QA:@=/%x /
 44 UA QA:@=/%x /
 55 UA QA:@=/%x /
 66 UA QA:@=/%x /
 77 UA QA:@=/%x /
